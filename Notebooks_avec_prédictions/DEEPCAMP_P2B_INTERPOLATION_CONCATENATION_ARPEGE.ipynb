        "id": "rEQ6rW9ynMFv"
      },
      "outputs": [],
      "source": [
        "class CombinedDataYears:\n",
        "\n",
        "    def __init__(self, df_x_forecast, df_stat_coord):\n",
        "        \"\"\"constuctor \n",
        "\n",
        "        Args:\n",
        "            df_x_forecast ([dataframe]): the dataframe of X_forecast data (arpege or arome)\n",
        "            df_stat_coord ([dataframe]): the dataframe of coordinate of stations \n",
        "        \"\"\"\n",
        "        self.df_x_forecast = df_x_forecast\n",
        "        self.df_stat_coord = df_stat_coord\n",
        "\n",
        "    def get_station_coord(self):\n",
        "        \"\"\"[summary]\n",
        "        \"\"\"\n",
        "\n",
        "        # this list will contain argument of stations that are close to the stations for which we have the Y to predict\n",
        "        liste_argmin = []\n",
        "        stat_coord = self.df_stat_coord[['lat', 'lon']].values\n",
        "        # get the distinct values of station coordinates\n",
        "        distinct_station = pd.DataFrame(np.unique(\n",
        "            self.df_x_forecast[['latitude', 'longitude']].values, axis=0), columns=[\"latitude\", 'longitude'])\n",
        "\n",
        "        for values_sta in tqdm(stat_coord):\n",
        "            liste_argmin.append(np.argmin(distinct_station[['latitude', 'longitude']].apply(\n",
        "                lambda x: haversine(x.latitude, x.longitude, values_sta[0], values_sta[1]), axis=1)))\n",
        "\n",
        "        # transform numpy array\n",
        "        liste_tuples = tuple(\n",
        "            map(tuple, distinct_station.iloc[liste_argmin].values))\n",
        "        # get stations that correspond to agmins (close to the station coordinates from arpege/arome)\n",
        "        coord_close_stat = distinct_station.iloc[liste_argmin].values\n",
        "\n",
        "        # get get station from apege/arome which are only close the station coordinates\n",
        "        df_result = self.df_x_forecast[self.df_x_forecast[[\n",
        "            'latitude', 'longitude']].apply(tuple, axis=1).isin(liste_tuples)]\n",
        "\n",
        "        # create a dataframe to merge dataframe in order to get the station number \n",
        "        dico = {\n",
        "            'lat_stat': list(stat_coord[0:, 0]), # the latitude of the station \n",
        "            'long_stat': list(stat_coord[0:, 1]),\n",
        "            'latitude': list(coord_close_stat[0:, 0]), # the latitude of the closest stations \n",
        "            'longitude': list(coord_close_stat[0:, 1]),\n",
        "            'number_sta': list(self.df_stat_coord['number_sta'])\n",
        "        }\n",
        "\n",
        "        df_coord_merged = pd.DataFrame(dico)\n",
        "\n",
        "        return pd.merge(df_result,  df_coord_merged, on=['latitude', 'longitude'], how='left')\n"
      ]
    },
